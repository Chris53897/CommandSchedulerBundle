{
    "name": "dukecity/command-scheduler-bundle",
    "description": "This Symfony bundle will allow you to schedule all your commands just like UNIX crontab",
    "keywords": ["Symfony", "scheduler", "cron", "command", "task"],
    "homepage": "https://github.com/Dukecity/CommandSchedulerBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Julien Guyon",
            "email": "julienguyon@hotmail.com"
        },
        {
            "name": "Dukecity",
            "email": "dukecity.code@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/console": "^4.4.20|^5.2",
        "doctrine/orm": "^2.8.1",
        "doctrine/doctrine-bundle": "^2.2",
        "dragonmantank/cron-expression": "^3.1",
        "symfony/framework-bundle": "^4.4.20|^5.2",
        "symfony/twig-bundle": "^4.4.20|^5.2",
        "symfony/translation": "^4.4.20|^5.2",
        "symfony/form": "^4.4.20|^5.2",
        "symfony/asset": "^4.4.20|^5.2",
        "symfony/templating": "^4.4.20|^5.2",
        "symfony/validator": "^4.4.20|^5.2",
        "ext-simplexml": "*",
        "ext-posix": "*",
        "symfony/notifier": "^5.2",
        "nesbot/carbon": "^2.43",
        "symfony/debug-bundle": "^4.4.20|^5.2",
        "symfony/dotenv": "^4.4.20|^5.2",
        "symfony/twig-pack": "^1.0",
        "twig/cssinliner-extra": "^3.0",
        "twig/inky-extra": "^3.0",
        "symfony/lock": "^4.4.20|^5.2",
        "sensio/framework-extra-bundle": "^6.1",
        "symfony/config": "^4.4.20|^5.2",
        "symfony/dependency-injection": "^4.4.20|^5.2",
        "symfony/property-access": "^4.4.20|^5.2",
        "symfony/http-kernel": "^4.4.20|^5.2",
        "symfony/inflector": "^4.4.20|^5.2",
        "symfony/yaml": "^4.4.20|^5.2",
        "symfony/service-contracts": "^2.2"
    },
    "require-dev": {
        "ext-pcntl": "*",
        "phpunit/phpunit": "^9.3",
        "doctrine/doctrine-fixtures-bundle": "^3.4.0",
        "symfony/dom-crawler": "^4.4.20|^5.2",
        "symfony/css-selector": "^4.4.20|^5.2",
        "symfony/security-bundle": "^4.4.20|^5.2",
        "liip/test-fixtures-bundle": "^1.11.0",
        "symfony/browser-kit": "^4.4.20|^5.2",
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "suggest": {
        "ext-pcntl": "For using the scheduler daemon"
    },
    "autoload": {
        "psr-4": {
            "Dukecity\\CommandSchedulerBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "Tests/"
        }
    }
}
